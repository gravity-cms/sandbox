<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Gravity\MediaBundle\Entity\GalleryHasMedia" table="media_gallery_has_media">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <many-to-one target-entity="Gravity\MediaBundle\Entity\Gallery" field="gallery" inversed-by="galleryHasMedias">
        </many-to-one>
        <many-to-one target-entity="Gravity\MediaBundle\Entity\Media" field="media" inversed-by="galleryHasMedias">
        </many-to-one>
    </entity>

</doctrine-mapping>
